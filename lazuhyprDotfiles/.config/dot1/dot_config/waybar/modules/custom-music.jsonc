{
	"custom/music": {
		"format": "{0}",
		"return-type": "json",
		"escape": true,
		"tooltip": true,
		"max-length": 40,
		"exec": " ~/.config/waybar/scripts/mediaplayer.py",
		"on-click": "playerctl --player spotify play-pause",
		"on-scroll-up": "playerctl next",
		"on-scroll-down": "playerctl previous"
	}
}
